{
  "name": "haruncpi/laravel-simple-filemanager",
  "description": "A simple filemanager for Laravel",
  "license": "cc-by-nc-4.0",
  "keywords": [
    "laravel",
    "simple",
    "filemanager",
    "file manager",
    "media manager"
  ],
  "type": "library",
  "authors": [
    {
      "name": "MD.HARUN-UR-RASHID",
      "email": "harun.cox@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Corebase\\Filemanager\\": "src"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Corebase\\Filemanager\\src\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
